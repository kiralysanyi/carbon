{
  "name": "sanyicraft-carbon",
  "displayName": "Carbon",
  "version": "0.3.28-beta.3",
  "version_index": 13,
  "description": "Web browser",
  "main": "index.js",
  "scripts": {
    "test": "electron . --carbon-debug --trace-warnings --verbose",
    "test-daemon": "electron . --carbon-debug --trace-warnings --verbose --nowindowinit",
    "test-setup": "electron . --carbon-debug --trace-warnings --verbose --setup",
    "autobuild": "electron-builder",
    "build_win": "electron-builder -w -p always",
    "build_linux": "electron-builder -l -p always",
    "build_lw": "electron-builder -lw -p always",
    "build_mac": "electron-builder -m -p always",
    "build_all": "electron-builder -lwm -p always",
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiralysanyi/carbon.git"
  },
  "keywords": [
    "web",
    "browser"
  ],
  "author": "Király Sándor",
  "license": "ISC",
  "homepage": "https://github.com/kiralysanyi/carbon",
  "devDependencies": {
    "electron": "github:castlabs/electron-releases#v24.5.0+wvcus",
    "electron-builder": "^23.6.0"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.26.6",
    "colorthief": "2.3.2",
    "cross-fetch": "3.1.5",
    "electron-context-menu": "3.5.0",
    "electron-updater": "5.3.0"
  }
}
